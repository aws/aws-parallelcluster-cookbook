# Generated by Chef for cfncluster <%= node['cfncluster']['cfn_node_type'] -%>
# Local modifications could be be overwritten.
<% case node['cfncluster']['cfn_node_type'] -%>
<%# MasterServer -%>
<% when 'MasterServer' -%>
[program:sqswatcher]
command = /usr/local/bin/sqswatcher
redirect_stderr = true
stdout_logfile = /var/log/sqswatcher

[program:jobwatcher]
command = /usr/local/bin/jobwatcher
redirect_stderr = true
stdout_logfile = /var/log/jobwatcher

<%# ComputeFleet -%>
<% when 'ComputeFleet' -%>
[program:nodewatcher]
command = /usr/local/bin/nodewatcher
redirect_stderr = true
stdout_logfile = /var/log/nodewatcher
<% end -%>
