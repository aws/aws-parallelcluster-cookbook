  "metric_configs": {
    "metrics_collected": [
      {
        "metric_type": "mem",
        "measurement": [
          "mem_used_percent"
        ],
        "metrics_collection_interval": 60,
        "node_roles": [
          "HeadNode"
        ]
      },
      {
        "metric_type": "disk",
        "measurement": [
          "disk_used_percent"
        ],
        "resources": [
          "/"
        ],
        "metrics_collection_interval": 60,
        "node_roles": [
          "HeadNode"
        ]
      }
    ],
    "append_dimensions": [
      "InstanceId"
    ],
    "aggregation_dimensions": [
      [
        "InstanceId",
        "path"
      ]
    ]
  }
